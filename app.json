{
  "expo": {
    "name": "Mind Your Task",
    "slug": "mind-your-task",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.cervere.research.mindyourtask",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4A90E2"
      },
      "package": "com.cervere.research.mindyourtask"
    },
    "plugins": [],
    "extra": {
      "apiUrl": "process.env.EXPO_PUBLIC_API_URL",
      "researcherPasscode": "process.env.EXPO_PUBLIC_RESEARCHER_PASSCODE",
      "eas": {
        "projectId": "36e495f9-64d5-4dca-9a2d-4017b98cf6e1"
      }
    },
    "owner": "cervere"
  }
}
